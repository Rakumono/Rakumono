#shop-carousel.carousel.slide(data-ride="carousel")
  %ol.carousel-indicators
    %li(data-target="#shop-carousel" data-slide-to="0")
    %li(data-target="#shop-carousel" data-slide-to="1")
    %li(data-target="#shop-carousel" data-slide-to="2")
  .carousel-inner(role="listbox")
    .item
      %img(src="http://img2.wikia.nocookie.net/__cb20120721135950/gintama/images/8/87/Sougo.png" alt="SB")
      .carousel-caption
        %h1 sb
        %h1 sb
    .item
      %img(src="http://img2.wikia.nocookie.net/__cb20120721135950/gintama/images/8/87/Sougo.png" alt="DB")
      .carousel-caption
        %h1 db
        %h1 db
        %h1 bitch
  %a.left.carousel-control(href="#carousel-example-generic" role="button" data-slide="prev")
    %span.glyphicon.glyphicon-chevron-left(aria-hidden="true")
    %span.sr-only Previous
  %a.right.carousel-control(href="#carousel-example-generic" role="button" data-slide="next")
    %span.glyphicon.glyphicon-chevron-right(aria-hidden="true")
    %span.sr-only Next


.row
  .col-lg-12
    %h1 萬屋
    .category-list
      %h2.category-title suiei
      .mono-list.row
    .category-list
      %h2.category-title boshi
      .mono-list
.shop-list.item
  .container.carousel-caption
  .row
    .col-lg-12
      %h1 萬屋
      .category-list
        %h2.category-title suiei
        .mono-list.row

:javascript
  $(".carousel").carousel();
  


