!!! 5
%html(lang="zh-cmn-Hans")
  %head
    %meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
    %link(rel="stylesheet" href="/vendors/bootstrap/css/bootstrap.min.css" )
    %link(href="/vendors/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet")
    %link(href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet")
    %link(rel="stylesheet" href="/styles/styles.css")
    %link(rel="stylesheet" href="/vendors/fullpage/jquery.fullPage.css")
    %script(src="/javascripts/jquery-1.11.1.min.js")
    %script(src="/javascripts/angular.min.js")  
    %script(src="/vendors/bootstrap/js/bootstrap.min.js")
    %script(src="/javascripts/scripts.js")

    %script(src="/vendors/bootstrap-tagsinput/bootstrap-tagsinput.min.js")
    %script(src="/vendors/jquery.easings.min.js")
    %script(src="/vendors/jquery.slimscroll.min.js")
    %script(src="/vendors/fullpage/jquery.fullPage.js")
    %title Index
  %body#page-top
    %nav.navbar.navbar-default.navbar-fixed-top
      .container
        .navbar-header.page-scroll
          %button.navbar-toggle(data-target="#bs-example-navbar-collapse-1" data-toggle="collapse" type="button")
          %span.sr-only RAKUMONO
          %a.navbar-brand(href="#page-top") RAKUMONO
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          %ul.nav.navbar-nav.navbar-right
            %li.hidden.active
            %li.page-scroll 
              %a(href="#") HOME
            %li.page-scroll 
              %a(href="#") APP
            %li.page-scroll 
              %a(href="#") CONTACT
    = yield
    
